    "def generate_decision_reasoning(country, risk_score, decision, priority):\n",
    "    \"\"\"Generate human-readable explanation for why a decision was made\"\"\"\n",
    "    \n",
    "    reasoning_parts = []\n",
    "    \n",
    "    # Risk assessment reasoning\n",
    "    if risk_score <= 3.0:\n",
    "        risk_reason = f\"{country} shows excellent stability with a low risk score of {risk_score:.1f}. The market environment is predictable with minimal barriers to entry.\"\n",
    "    elif risk_score <= 4.0:\n",
    "        risk_reason = f\"{country} presents a favorable environment with a manageable risk score of {risk_score:.1f}. Most challenges can be addressed through standard business planning.\"\n",
    "    elif risk_score <= 6.0:\n",
    "        risk_reason = f\"{country} has moderate challenges with a risk score of {risk_score:.1f}. Success is achievable but requires careful attention to regulatory and market dynamics.\"\n",
    "    elif risk_score <= 8.0:\n",
    "        risk_reason = f\"{country} presents significant challenges with a high risk score of {risk_score:.1f}. Entry would require substantial resources and local expertise.\"\n",
    "    else:\n",
    "        risk_reason = f\"{country} shows critical risk factors with a score of {risk_score:.1f}. The market environment presents major obstacles that could jeopardize success.\"\n",
    "    \n",
    "    reasoning_parts.append(risk_reason)\n",
    "    \n",
    "    # Decision-specific reasoning\n",
    "    if decision == \"PROCEED_WITH_CONFIDENCE\":\n",
    "        decision_reason = \"The combination of strong market opportunity and low risk makes this ideal for immediate market entry. The business environment supports sustainable growth.\"\n",
    "    elif decision == \"PROCEED_WITH_CAUTION\":\n",
    "        decision_reason = \"While the market shows promise, certain risk factors require careful management. A phased entry approach with local partnerships is recommended.\"\n",
    "    elif decision == \"PROCEED_WITH_MITIGATION\":\n",
    "        decision_reason = \"The market opportunity exists but comes with substantial risks. Success depends on implementing comprehensive risk mitigation strategies.\"\n",
    "    else:  # RECONSIDER_ENTRY\n",
    "        decision_reason = \"The high risk environment outweighs potential benefits. Market entry could result in significant losses or operational difficulties.\"\n",
    "    \n",
    "    reasoning_parts.append(decision_reason)\n",
    "    \n",
    "    # Priority reasoning\n",
    "    if priority == \"High\":\n",
    "        priority_reason = \"This market should be prioritized in your expansion strategy due to its attractive risk-reward profile.\"\n",
    "    elif priority == \"Medium\":\n",
    "        priority_reason = \"This market represents a solid opportunity to consider after addressing higher-priority markets.\"\n",
    "    elif priority == \"Low\":\n",
    "        priority_reason = \"While not immediately urgent, this market could be valuable for future expansion.\"\n",
    "    else:\n",
    "        priority_reason = \"This market should not be a focus for current expansion efforts.\"\n",
    "    \n",
    "    reasoning_parts.append(priority_reason)\n",
    "    \n",
    "    return \" \".join(reasoning_parts)\n",
    "\n",
    "def benchmark_agents():\n",
    "    \"\"\"Benchmark all agents with decision reasoning\"\"\"\n",
    "    print(\"📊 Running Performance Benchmark...\")\n",
    "    \n",
    "    countries = [\"Germany\", \"Japan\", \"United Kingdom\", \"France\", \"Canada\"]\n",
    "    results = []\n",
    "    \n",
    "    market_agent = MarketResearchAgent()\n",
    "    risk_agent = RiskAssessmentAgent()\n",
    "    orchestrator = MultiAgentOrchestrator()\n",
    "    \n",
    "    for country in countries:\n",
    "        print(f\"\\n🔍 Testing {country}...\")\n",
    "        \n",
    "        # Test agents\n",
    "        start_time = time.time()\n",
    "        market_result = market_agent.analyze_market(country, \"technology\")\n",
    "        market_time = time.time() - start_time\n",
    "        \n",
    "        start_time = time.time()\n",
    "        risk_result = risk_agent.comprehensive_risk_assessment(country, \"technology\")\n",
    "        risk_time = time.time() - start_time\n",
    "        \n",
    "        start_time = time.time()\n",
    "        orchestration_result = orchestrator.comprehensive_market_entry_analysis(country, \"technology\")\n",
    "        orchestration_time = time.time() - start_time\n",
    "        \n",
    "        # Extract key metrics\n",
    "        risk_score = risk_result['overall_risk_score']\n",
    "        decision = orchestration_result['recommendation']['decision']\n",
    "        priority = orchestration_result['recommendation']['priority']\n",
    "        \n",
    "        # Create visual summary\n",
    "        if risk_score <= 4.0:\n",
    "            risk_emoji = \"🟢\"\n",
    "            risk_summary = \"Low Risk\"\n",
    "        elif risk_score <= 6.0:\n",
    "            risk_emoji = \"🟡\"\n",
    "            risk_summary = \"Medium Risk\"\n",
    "        elif risk_score <= 8.0:\n",
    "            risk_emoji = \"🟠\"\n",
    "            risk_summary = \"High Risk\"\n",
    "        else:\n",
    "            risk_emoji = \"🔴\"\n",
    "            risk_summary = \"Critical Risk\"\n",
    "        \n",
    "        priority_emojis = {\"High\": \"🚀\", \"Medium\": \"⚖️\", \"Low\": \"📋\", \"Very Low\": \"⏸️\"}\n",
    "        priority_emoji = priority_emojis.get(priority, \"❓\")\n",
    "        \n",
    "        decision_short = decision.replace(\"PROCEED_WITH_\", \"\").replace(\"_\", \" \").title()\n",
    "        if \"RECONSIDER\" in decision:\n",
    "            decision_short = \"Reconsider\"\n",
    "        \n",
    "        brief_summary = f\"{risk_emoji} {risk_summary} | {priority_emoji} {priority} Priority | {decision_short}\"\n",
    "        \n",
    "        results.append({\n",
    "            'country': country,\n",
    "            'risk_score': risk_score,\n",
    "            'risk_level': risk_result['risk_level'],\n",
    "            'final_decision': decision,\n",
    "            'priority': priority,\n",
    "            'brief_summary': brief_summary,\n",
    "            'market_agent_time': market_time,\n",
    "            'risk_agent_time': risk_time,\n",
    "            'orchestration_time': orchestration_time\n",
    "        })\n",
    "        \n",
    "        print(f\"   Decision: {decision_short} | Risk: {risk_score:.1f} | Priority: {priority}\")\n",
    "    \n",
    "    return pd.DataFrame(results)\n",
    "\n",
    "# Run benchmark\n",
    "benchmark_df = benchmark_agents()\n",
    "\n",
    "print(\"\\n📊 Benchmark Results Summary:\")\n",
    "print(\"=\"*80)\n",
    "print(\"\\n🎯 **Market Entry Recommendations:**\")\n",
    "for _, row in benchmark_df.iterrows():\n",
    "    print(f\"   {row['country']:<15} | Risk: {row['risk_score']:<4.1f} | {row['brief_summary']}\")"
   ]
