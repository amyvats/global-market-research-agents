    "# Install PDF generation libraries\n",
    "!pip install -q reportlab matplotlib seaborn\n",
    "\n",
    "from reportlab.lib.pagesizes import letter, A4\n",
    "from reportlab.platypus import SimpleDocTemplate, Paragraph, Spacer, Image, Table, TableStyle, PageBreak\n",
    "from reportlab.lib.styles import getSampleStyleSheet, ParagraphStyle\n",
    "from reportlab.lib.units import inch\n",
    "from reportlab.lib import colors\n",
    "from reportlab.lib.enums import TA_CENTER, TA_LEFT\n",
    "import os\n",
    "\n",
    "def generate_benchmark_pdf_report(benchmark_df, output_filename=None):\n",
    "    \"\"\"Generate PDF report from benchmark results\"\"\"\n",
    "    \n",
    "    if output_filename is None:\n",
    "        output_filename = f\"Agent_Benchmark_Report_{datetime.now().strftime('%Y%m%d_%H%M%S')}.pdf\"\n",
    "    \n",
    "    # Setup styles\n",
    "    styles = getSampleStyleSheet()\n",
    "    title_style = ParagraphStyle(\n",
    "        'CustomTitle',\n",
    "        parent=styles['Heading1'],\n",
    "        fontSize=24,\n",
    "        spaceAfter=30,\n",
    "        alignment=TA_CENTER,\n",
    "        textColor=colors.darkblue\n",
    "    )\n",
    "    \n",
    "    heading_style = ParagraphStyle(\n",
    "        'CustomHeading',\n",
    "        parent=styles['Heading2'],\n",
    "        fontSize=16,\n",
    "        spaceAfter=12,\n",
    "        textColor=colors.darkblue\n",
    "    )\n",
    "    \n",
    "    body_style = ParagraphStyle(\n",
    "        'CustomBody',\n",
    "        parent=styles['Normal'],\n",
    "        fontSize=11,\n",
    "        spaceAfter=6\n",
    "    )\n",
    "    \n",
    "    # Create PDF document\n",
    "    doc = SimpleDocTemplate(output_filename, pagesize=A4)\n",
    "    story = []\n",
    "    \n",
    "    # Title Page\n",
    "    story.append(Paragraph(\"Market Research Agents\", title_style))\n",
    "    story.append(Paragraph(\"Benchmark Testing Report\", title_style))\n",
    "    story.append(Spacer(1, 0.5*inch))\n",
    "    \n",
    "    # Report metadata\n",
    "    report_date = datetime.now().strftime(\"%B %d, %Y at %I:%M %p\")\n",
    "    story.append(Paragraph(f\"<b>Report Generated:</b> {report_date}\", body_style))\n",
    "    story.append(Paragraph(f\"<b>Countries Analyzed:</b> {len(benchmark_df)}\", body_style))\n",
    "    story.append(Paragraph(f\"<b>Agents Tested:</b> Market Research, Risk Assessment, Multi-Agent Orchestrator\", body_style))\n",
    "    story.append(PageBreak())\n",
    "    \n",
    "    # Executive Summary\n",
    "    story.append(Paragraph(\"Executive Summary\", heading_style))\n",
    "    \n",
    "    avg_risk = benchmark_df['risk_score'].mean()\n",
    "    avg_market_time = benchmark_df['market_agent_time'].mean()\n",
    "    avg_risk_time = benchmark_df['risk_agent_time'].mean()\n",
    "    avg_orchestration_time = benchmark_df['orchestration_time'].mean()\n",
    "    \n",
    "    # Overall assessment\n",
    "    if avg_risk <= 4.0:\n",
    "        risk_assessment = \"<font color='green'><b>EXCELLENT</b></font> - Markets show low risk profiles suitable for expansion\"\n",
    "    elif avg_risk <= 6.0:\n",
    "        risk_assessment = \"<font color='orange'><b>GOOD</b></font> - Markets have manageable risk levels with proper planning\"\n",
    "    else:\n",
    "        risk_assessment = \"<font color='red'><b>CAUTION</b></font> - Markets present significant challenges requiring careful consideration\"\n",
    "    \n",
    "    story.append(Paragraph(f\"<b>Overall Risk Assessment:</b> {risk_assessment}\", body_style))\n",
    "    story.append(Paragraph(f\"<b>Average Risk Score:</b> {avg_risk:.2f}/10\", body_style))\n",
    "    story.append(Paragraph(f\"<b>Performance:</b> All agents performed within acceptable response times\", body_style))\n",
    "    story.append(Spacer(1, 0.2*inch))\n",
    "    \n",
    "    # Key Findings\n",
    "    best_market = benchmark_df.loc[benchmark_df['risk_score'].idxmin()]\n",
    "    worst_market = benchmark_df.loc[benchmark_df['risk_score'].idxmax()]\n",
    "    high_priority_count = len(benchmark_df[benchmark_df['priority'] == 'High'])\n",
    "    \n",
    "    story.append(Paragraph(\"Key Findings\", heading_style))\n",
    "    findings = [\n",
    "        f\"• <b>Lowest Risk Market:</b> {best_market['country']} (Risk Score: {best_market['risk_score']:.2f})\",\n",
    "        f\"• <b>Highest Risk Market:</b> {worst_market['country']} (Risk Score: {worst_market['risk_score']:.2f})\",\n",
    "        f\"• <b>High Priority Markets:</b> {high_priority_count} out of {len(benchmark_df)} countries\",\n",
    "        f\"• <b>Average Response Time:</b> Market Agent ({avg_market_time:.1f}s), Risk Agent ({avg_risk_time:.1f}s)\",\n",
    "        f\"• <b>System Performance:</b> All agents operational and responsive\"\n",
    "    ]\n",
    "    \n",
    "    for finding in findings:\n",
    "        story.append(Paragraph(finding, body_style))\n",
    "    \n",
    "    story.append(PageBreak())\n",
    "    \n",
    "    # Detailed Results Table\n",
