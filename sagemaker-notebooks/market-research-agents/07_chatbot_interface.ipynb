    "    story.append(PageBreak())\n",
    "    \n",
    "    # Executive Summary\n",
    "    story.append(Paragraph(\"Executive Summary\", heading_style))\n",
    "    \n",
    "    # Calculate metrics\n",
    "    total_tests = len(comprehensive_results)\n",
