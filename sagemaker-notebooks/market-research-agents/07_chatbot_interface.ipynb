    "        status_color = 'red'\n",
    "        status_text = \"NEEDS IMPROVEMENT - Some functionality issues\"\n",
    "    else:\n",
    "        status_color = 'darkred'\n",
    "        status_text = \"POOR - Significant issues need addressing\"\n",
    "    \n",
    "    story.append(Paragraph(f\"<b>Overall Chatbot Score:</b> <font color='{status_color}'>{overall_score*100:.1f}%</font>\", body_style))\n",
    "    story.append(Paragraph(f\"<b>Status:</b> <font color='{status_color}'>{status_text}</font>\", body_style))\n",
    "    story.append(Paragraph(f\"<b>Average Response Time:</b> {avg_response_time:.2f} seconds\", body_style))\n",
    "    story.append(Spacer(1, 0.2*inch))\n",
    "    \n",
    "    # Key Findings\n",
    "    story.append(Paragraph(\"Key Findings\", heading_style))\n",
    "    \n",
    "    findings = [\n",
    "        f\"• <b>Functionality Tests:</b> {passed_tests}/{total_tests} passed ({passed_tests/total_tests*100:.1f}%)\",\n",
    "        f\"• <b>Edge Case Handling:</b> {handled_gracefully}/{total_edge} handled gracefully ({handled_gracefully/total_edge*100:.1f}%)\",\n",
    "        f\"• <b>Conversation Flow:</b> {successful_turns}/{total_turns} turns successful ({successful_turns/total_turns*100:.1f}%)\",\n",
    "        f\"• <b>Response Time:</b> Average {avg_response_time:.2f}s ({'Excellent' if avg_response_time < 30 else 'Good' if avg_response_time < 60 else 'Needs optimization'})\",\n",
    "        f\"• <b>Intent Recognition:</b> {'Excellent' if passed_tests/total_tests > 0.8 else 'Good' if passed_tests/total_tests > 0.6 else 'Needs improvement'}\"\n",
    "    ]\n",
    "    \n",
    "    for finding in findings:\n",
    "        story.append(Paragraph(finding, body_style))\n",
    "    \n",
    "    story.append(PageBreak())\n",
    "    \n",
    "    # Detailed Test Results Table\n",
    "    story.append(Paragraph(\"Detailed Test Results\", heading_style))\n",
    "    \n",
    "    # Comprehensive Tests Table\n",
    "    table_data = [['Test Category', 'Query Sample', 'Response Type', 'Success', 'Response Time (s)']]\n",
