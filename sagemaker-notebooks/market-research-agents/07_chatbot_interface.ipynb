    "            row['test_case'],\n",
    "            query_sample,\n",
    "            row['response_type'],\n",
    "            success_icon,\n",
    "            f\"{row['response_time']:.2f}\"\n",
    "        ])\n",
    "    \n",
    "    table = Table(table_data, colWidths=[1.5*inch, 2.5*inch, 1.2*inch, 0.6*inch, 1*inch])\n",
    "    table.setStyle(TableStyle([\n",
    "        ('BACKGROUND', (0, 0), (-1, 0), colors.darkblue),\n",
    "        ('TEXTCOLOR', (0, 0), (-1, 0), colors.whitesmoke),\n",
    "        ('ALIGN', (0, 0), (-1, -1), 'CENTER'),\n",
    "        ('FONTNAME', (0, 0), (-1, 0), 'Helvetica-Bold'),\n",
    "        ('FONTSIZE', (0, 0), (-1, 0), 9),\n",
    "        ('BOTTOMPADDING', (0, 0), (-1, 0), 12),\n",
    "        ('BACKGROUND', (0, 1), (-1, -1), colors.beige),\n",
    "        ('FONTSIZE', (0, 1), (-1, -1), 8),\n",
    "        ('GRID', (0, 0), (-1, -1), 1, colors.black)\n",
    "    ]))\n",
    "    \n",
    "    story.append(table)\n",
    "    story.append(Spacer(1, 0.3*inch))\n",
    "    \n",
    "    # Recommendations\n",
    "    story.append(Paragraph(\"Recommendations\", heading_style))\n",
    "    \n",
    "    recommendations = [\n",
    "        \"<b>Immediate Actions:</b>\",\n",
    "        f\"• {'Deploy to production' if overall_score >= 0.9 else 'Address failing test cases before deployment'}\",\n",
    "        f\"• {'Monitor performance in production' if overall_score >= 0.8 else 'Improve response time optimization'}\",\n",
    "        f\"• {'Expand test coverage' if overall_score >= 0.7 else 'Fix critical functionality issues'}\",\n",
