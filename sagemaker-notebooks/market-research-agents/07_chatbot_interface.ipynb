    "def format_response(response: Dict[str, Any]) -> None:\n",
    "    \"\"\"Format and display chatbot response\"\"\"\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*60)\n",
    "    print(f\"🤖 {response['message']}\")\n",
    "    print(\"=\"*60)\n",
    "    \n",
    "    if response[\"response_type\"] == \"risk_assessment\":\n",
    "        print(f\"\\n⚠️ **Risk Assessment Results:**\")\n",
    "        print(f\"   🎯 Overall Risk Score: {response['risk_score']:.2f}/10\")\n",
    "        print(f\"   📊 Risk Level: {response['risk_level']}\")\n",
    "        print(f\"   🌍 Country: {response['country']}\")\n",
    "        print(f\"   🏭 Industry: {response['industry']}\")\n",
    "        \n",
    "        print(f\"\\n📋 **Risk Breakdown:**\")\n",
    "        for risk_type, score in response['risk_breakdown'].items():\n",
    "            print(f\"   {risk_type.title()}: {score:.2f}/10\")\n",
    "        \n",
    "        print(f\"\\n📝 **Detailed Analysis:**\")\n",
    "        print(f\"{response['detailed_analysis'][:500]}...\")\n",
    "    \n",
    "    elif response[\"response_type\"] == \"market_research\":\n",
    "        print(f\"\\n📊 **Market Research Results:**\")\n",
    "        print(f\"   🌍 Country: {response['country']}\")\n",
    "        print(f\"   🏭 Industry: {response['industry']}\")\n",
    "        print(f\"\\n📝 **Market Analysis:**\")\n",
    "        print(f\"{response['analysis'][:800]}...\")\n",
    "    \n",
    "    elif response[\"response_type\"] == \"comparison\":\n",
    "        print(f\"\\n📊 **Market Comparison Results:**\")\n",
    "        print(f\"   🏭 Industry: {response['industry']}\")\n",
    "        \n",
    "        # Create comparison table\n",
    "        df = pd.DataFrame(response['comparisons'])\n",
    "        print(f\"\\n📋 **Comparison Table:**\")\n",
    "        print(df.to_string(index=False))\n",
    "        \n",
    "        # Recommend best option\n",
    "        best_country = df.loc[df['risk_score'].idxmin(), 'country']\n",
    "        print(f\"\\n🏆 **Recommendation:** {best_country} appears to have the lowest risk profile\")\n",
    "    \n",
    "    elif response[\"response_type\"] == \"recommendation\":\n",
    "        print(f\"\\n🎯 **Market Entry Recommendation:**\")\n",
    "        print(f\"   🌍 Country: {response['country']}\")\n",
    "        print(f\"   🏭 Industry: {response['industry']}\")\n",
    "        print(f\"   📊 Decision: {response['decision']}\")\n",
    "        print(f\"   🎯 Priority: {response['priority']}\")\n",
    "        print(f\"   💯 Confidence: {response['confidence']}\")\n",
    "        \n",
    "        risk_data = response['risk_assessment']\n",
    "        print(f\"   ⚠️ Risk Score: {risk_data['overall_risk_score']:.2f}/10\")\n",
    "        print(f\"   📈 Risk Level: {risk_data['risk_level']}\")\n",
    "    \n",
    "    elif response[\"response_type\"] == \"comprehensive\":\n",
    "        full_data = response['full_analysis']\n",
    "        print(f\"\\n📊 **Comprehensive Analysis:**\")\n",
    "        print(f\"   🌍 Country: {response['country']}\")\n",
    "        print(f\"   🏭 Industry: {response['industry']}\")\n",
    "        print(f\"   📊 Final Decision: {full_data['recommendation']['decision']}\")\n",
    "        print(f\"   🎯 Priority: {full_data['recommendation']['priority']}\")\n",
    "        print(f\"   ⚠️ Risk Score: {full_data['recommendation']['risk_score']:.2f}/10\")\n",
    "    \n",
    "    elif response[\"response_type\"] == \"error\":\n",
    "        print(f\"\\n❌ **Error:** {response['message']}\")\n",
    "        print(f\"\\n💡 **Suggestions:**\")\n",
    "        for suggestion in response['suggestions']:\n",
    "            print(f\"   • {suggestion}\")\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*60)\n",
    "\n",
    "def chat_session():\n",
    "    \"\"\"Run interactive chat session\"\"\"\n",
    "    print(\"\\n🤖 Market Research Chatbot is ready!\")\n",
    "    print(\"💡 Type 'quit', 'exit', or 'bye' to end the conversation\")\n",
    "    print(\"📝 Type 'help' for example questions\")\n",
    "    \n",
    "    while True:\n",
    "        try:\n",
    "            # Get user input\n",
    "            user_input = input(\"\\n👤 You: \").strip()\n",
    "            \n",
    "            # Check for exit commands\n",
    "            if user_input.lower() in ['quit', 'exit', 'bye', 'goodbye']:\n",
    "                print(\"\\n🤖 Goodbye! Thanks for using the Market Research Chatbot!\")\n",
    "                break\n",
    "            \n",
    "            # Check for help\n",
    "            if user_input.lower() in ['help', '?']:\n",
    "                print(\"\\n💡 **Example Questions:**\")\n",
    "                print(\"   • What are the risks of entering the German fintech market?\")\n",
    "                print(\"   • Compare technology markets in Japan vs UK\")\n",
    "                print(\"   • Should I expand my e-commerce business to France?\")\n",
    "                print(\"   • What's the market size for AI in Singapore?\")\n",
    "                print(\"   • Is it safe to enter the healthcare market in Canada?\")\n",
    "                continue\n",
    "            \n",
    "            # Skip empty input\n",
    "            if not user_input:\n",
    "                continue\n",
    "            \n",
    "            # Process the query\n",
    "            print(\"\\n🤖 Analyzing your question...\")\n",
    "            response = chatbot.handle_query(user_input)\n",
    "            \n",
    "            # Display the response\n",
    "            format_response(response)\n",
    "            \n",
    "        except KeyboardInterrupt:\n",
    "            print(\"\\n\\n🤖 Goodbye! Thanks for using the Market Research Chatbot!\")\n",
    "            break\n",
    "        except Exception as e:\n",
    "            print(f\"\\n❌ An error occurred: {str(e)}\")\n",
    "            print(\"💡 Please try asking your question differently.\")\n",
    "\n",
    "print(\"✅ Chat interface ready! Run chat_session() to start chatting.\")\n\n# 🧪 CHATBOT TESTING FUNCTIONS\ndef test_chatbot_simple():\n    \"\"\"Simple chatbot testing function\"\"\"\n    \n    print(\"🧪 **TESTING CHATBOT CAPABILITIES**\")\n    print(\"=\" * 40)\n    \n    # Test queries\n    test_queries = [\n        \"What are the risks of entering the German fintech market?\",\n        \"Should I expand my technology business to Japan?\",\n        \"Compare e-commerce opportunities in UK vs France\",\n        \"Is it safe to invest in healthcare in Canada?\"\n    ]\n    \n    for i, query in enumerate(test_queries, 1):\n        print(f\"\\n🔍 **Test {i}: {query}**\")\n        \n        try:\n            # Parse the query\n            parsed = chatbot.parse_query(query)\n            \n            print(f\"   🌍 Countries: {parsed['countries'] if parsed['countries'] else 'None detected'}\")\n            print(f\"   🏭 Industries: {parsed['industries'] if parsed['industries'] else 'None detected'}\")\n            print(f\"   🎯 Intent: {parsed['intent']}\")\n            print(f\"   ✅ SUCCESS: Query parsed successfully!\")\n            \n        except Exception as e:\n            print(f\"   ❌ ERROR: {str(e)}\")\n    \n    print(f\"\\n🎉 **Chatbot testing completed!**\")\n\ndef add_global_countries():\n    \"\"\"Add more countries to chatbot support\"\"\"\n    \n    new_countries = [\n        \"Rwanda\", \"Kenya\", \"Ghana\", \"Nigeria\", \"Egypt\", \"Morocco\",\n        \"Bangladesh\", \"Nepal\", \"Myanmar\", \"Sri Lanka\", \"Mongolia\",\n        \"Estonia\", \"Latvia\", \"Lithuania\", \"Slovenia\", \"Malta\",\n        \"Ecuador\", \"Uruguay\", \"Costa Rica\", \"Panama\", \"Jamaica\",\n        \"Fiji\", \"Samoa\", \"Tonga\", \"Vanuatu\", \"Papua New Guinea\"\n    ]\n    \n    # Add to chatbot countries\n    original_count = len(chatbot.countries)\n    chatbot.countries.extend(new_countries)\n    \n    print(f\"🌍 **GLOBAL COUNTRIES ADDED!**\")\n    print(f\"✅ Before